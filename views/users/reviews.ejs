<h1>Create new track note:</h1>
<form method="POST" action="/albums/<%= album.id %>/reviews">
    <label for="song">Track title</label>
    <input type="text" id="song" name="song" />
    <label for="rating">Rating 1-10</label>
    <input type="text" id="rating" name="rating" />
    
    <input type="submit" />
</form>


<h2><%= album.title %></h2>
<p>by: <%= album.artist %></p>
<h3>Favorite tracks:</h3>

<% reviews.forEach(review => { %>
    <h2><%= review.song %></h2>
    <p><%= review.rating %></p> 
    <% }) %>